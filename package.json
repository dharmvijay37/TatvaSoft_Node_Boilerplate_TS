{
  "name": "node-boiler-plate",
  "version": "1.0.0",
  "description": "\"# node-boiler-plate\"",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "swagger": "ts-node-dev ./src/swagger/generate-swagger.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "dev": "ts-node-dev --no-notify --clear --debounce 1000 ./src/server.ts",
    "prod": "node ./build/server.js",
    "swagger:spec": "tsoa spec"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
