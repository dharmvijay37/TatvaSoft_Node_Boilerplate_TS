    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.0",
    "@types/sequelize": "^4.28.13",
    "@types/swagger-jsdoc": "^6.0.1",
    "bcrypt": "^5.0.1",
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-throttle": "^2.0.0",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.20.1",
    "sequelize-cli": "^6.4.1",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.5.0",
    "ts-node-dev": "^2.0.0",
    "tsoa": "^4.1.0",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.2.0",
    "@types/jest": "^28.1.1",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "concurrently": "^7.2.2",
    "eslint": "^8.17.0",
